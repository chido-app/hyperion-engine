sign: Function = (num: Number) {
  if num > 0 {
    return 1
  }
  if num < 0 {
    return -1
  }
  return 0
}

abs: Function = (num: Number) {
  if num < 0 {
    return -num
  }
  return num
}

floor: Function = (num: Number) {
  if num >= 0 {
    return num - (num % 1)
  }
  return num - 1 - (num % 1)
}

ceil: Function = (num: Number) {
  if num >= 0 {
    return num + 1 - (num % 1)
  }
  return num - (num % 1)
}